{
  "name": "node-starter",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/server.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register ./src/server.ts",
    "build": "tsc",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.(ts)": [
      "prettier --write",
      "eslint"
    ],
    "!*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "mongoose": "^8.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.13",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.28.0"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">= 22.13.0",
    "yarn": "please-use-pnpm",
    "pnpm": ">= 9.5.0"
  }
}
